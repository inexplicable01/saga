/*-----QLabel-----*/
QLabel
{
	background-color: transparent;
	color: #fff;
	font-size: 14pt;
	border-color: #000000;
	border-radius: 1px

}


/*-----QToolButton-----*/
QToolButton
{
	background-color: #BB86FC;
	color: #000;
	font-size: 14pt;
	border-radius: 4px;
	padding: 10px;

}


QToolButton::pressed
{
	background-color: #6b960f;

}




